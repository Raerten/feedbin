<%= render(Form::TextInputComponent.new) do |text| %>
  <% text.input do %>
    <input type="search">
  <% end %>
  <% text.accessory_leading do %>
    <%= svg_tag "icon-search", class: "fill-400" %>
  <% end %>
  <% text.accessory_trailing position: "trailing" do %>
    <%= svg_tag "icon-close", class: "fill-400" %>
  <% end %>
<% end %>
