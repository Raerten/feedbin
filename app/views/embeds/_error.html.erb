<div class="iframe-embed system-content" data-behavior="iframe_placeholder" data-iframe-src="<%= @url %>" data-iframe-width="<%= params[:width] %>" data-iframe-height="<%= params[:height] %>" title="Load embed">
    <div class="embed-profile entry-callout">
        <span class="embed-data">
            <span class="embed-title">Load embed</span>
            <span class="embed-subtitle"><%= @host %></span>
        </span>
        <a class="embed-icon embed-link" href="<%= @url %>">
            <%= favicon_with_host(@host) %>
            <%= svg_tag "icon-embed-source-default" %>
        </a>
    </div>
</div>
