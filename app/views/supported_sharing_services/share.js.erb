<% if @response[:error].present? %>
    feedbin.showNotification("<%= j @response[:error] %>", true, "<%= j @response[:url] %>");
<% elsif @response[:message].present? %>
    feedbin.showNotification("<%= j @response[:message] %>", false, "<%= j @response[:url] %>");
<% else %>
    <%= @response[:text].html_safe %>
<% end %>
